{
  "name": "yor.ts",
  "version": "0.0.7",
  "description": "A highly performant and typed library to make discord http based bots",
  "keywords": [
    "discord",
    "discordjs",
    "discord-interactions",
    "cloudflare-workers",
    "wrapper"
  ],
  "homepage": "https://yor.mintlify.app",
  "bugs": {
    "url": "https://github.com/OreOreki/yor.ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OreOreki/yor.ts.git"
  },
  "license": "MIT",
  "author": "OreOreki (Oreki)",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./builders": {
      "require": "./dist/builders/index.js",
      "import": "./dist/builders/index.mjs",
      "types": "./dist/builders/index.d.ts"
    },
    "./util": {
      "require": "./dist/util/index.js",
      "import": "./dist/util/index.mjs",
      "types": "./dist/util/index.d.ts"
    },
    "./core": {
      "require": "./dist/core/index.js",
      "import": "./dist/core/index.mjs",
      "types": "./dist/core/index.d.ts"
    },
    "./rest": {
      "require": "./dist/rest/index.js",
      "import": "./dist/rest/index.mjs",
      "types": "./dist/rest/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "yor": "./dist/cli/index.mjs"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsup ./src",
    "coverage": "nyc report --reporter=json -m",
    "docs:complete": "tsx scripts/mintlify.ts",
    "docs:generate": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-plugin-mdn-links --plugin typedoc-plugin-djs-links --out temp src",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "prepublishOnly": "pnpm run build",
    "tests": "nyc mocha tests/**/*.ts -r tsx --extension ts",
    "typecheck": "tsc --noEmit",
    "version": "auto-changelog -p"
  },
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@discordjs/collection": "^2.0.0",
    "@discordjs/core": "^1.1.1",
    "@discordjs/formatters": "^0.3.3",
    "@discordjs/rest": "^2.2.0",
    "@discordjs/util": "^1.0.2",
    "discord-interactions": "^3.4.0"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/sinon": "^17.0.2",
    "@vercel/style-guide": "^5.1.0",
    "auto-changelog": "^2.4.0",
    "chai": "^4.3.10",
    "eslint": "^8.55.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "picocolors": "^1.0.0",
    "prettier": "^3.1.1",
    "sinon": "^17.0.1",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typedoc": "^0.25.4",
    "typedoc-plugin-djs-links": "^2.2.0",
    "typedoc-plugin-markdown": "^3.17.1",
    "typedoc-plugin-mdn-links": "^3.1.7",
    "typescript": "^5.3.3"
  }
}
